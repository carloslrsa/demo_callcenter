<?xml version="1.0"?>
<odoo>
	<record id="reporte_queja_form_view" model="ir.ui.view">
		<field name="name">reporte.queja.form.view</field>
		<field name="model">reporte.queja</field>
		<field name="arch" type="xml">
			<form string="Form Reporte Queja">
				<sheet>
					<group>
						<field name="id"/>
						<field name="tipo"/>
						<field name="nombres"/>
						<field name="direccion"/>
						<field name="telefono"/>
						<field name="email"/>
						<field name="estado"/>
					</group>
				</sheet>
			</form>
		</field>
	</record>

	<record id="reporte_queja_tree_view" model="ir.ui.view">
		<field name="name">reporte.queja.tree.view</field>
		<field name="model">reporte.queja</field>
		<field name="arch" type="xml">
			<tree string="Tree reporte_queja">
				<field name="nombres"/>
				<field name="email"/>
			</tree>
		</field>
	</record>

	<act_window id="action_reporte_queja" model="ir.actions.act_window">
		<field name="name">action.reporte.queja</field>
		<field name="type">ir.actions.act_window</field>
		<field name="res_model">reporte.queja</field>
		<field name="view_mode">tree,form</field>
	</act_window>

	<menuitem id="reporte_menu_root" name="ReporteQueja" sequence="1"/>
	<menuitem id="reporte_queja_menu_category" name="Queja" sequence="1" parent="reporte_menu_root"/>
	<menuitem id="action_reporte_queja_menu" name="Queja" sequence="1" parent="reporte_queja_menu_category" action="action_reporte_queja"/>
</odoo>